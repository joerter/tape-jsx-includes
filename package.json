{
  "name": "tape-jsx-includes",
  "version": "1.1.0",
  "description": "tape extension to assert one jsx string includes another",
  "main": "dist/index.js",
  "scripts": {
    "test": "tape -r babel-register ./test/index.js | faucet",
    "lint": "eslint src/ test/",
    "build": "babel -d dist/ src/",
    "clean": "rm -r node_modules dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joerter/tape-jsx-includes.git"
  },
  "keywords": [
    "tape",
    "react",
    "jsx"
  ],
  "author": "John Oerter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joerter/tape-jsx-includes/issues"
  },
  "homepage": "https://github.com/joerter/tape-jsx-includes#readme",
  "dependencies": {
    "collapse-white-space": "^1.0.0",
    "react-element-to-jsx-string": "^2.4.0"
  },
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "extend-tape": "^1.1.0",
    "faucet": "0.0.1",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "tape": "^4.4.0"
  }
}
